{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "space-before-function-paren": 0,
    "react/jsx-boolean-value": 0,
    "import/first": 0,
    "arrow-spacing": 0,
    "spaced-comment": 0,
    "curly":0,
    "semi":0,
    "no-trailing-spaces":0
  }
}
